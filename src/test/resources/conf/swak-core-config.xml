      <webModule>yuyu.testinfr.inject.BosyuuMockModule</webModule>
      <webModule>yuyu.testinfr.inject.DirectMockModule</webModule>
      <webModule>yuyu.testinfr.inject.SuuriMockModule</webModule>
      <webModule>yuyu.testinfr.inject.AuthMockModule</webModule>

