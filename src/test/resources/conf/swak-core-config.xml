      <webModule>jp.co.slcs.swak.core.inject.SWAKLoggerModule</webModule>
      <webModule>jp.co.slcs.swak.core.inject.SWAKCoreModule</webModule>
      <webModule>jp.co.slcs.swak.bizinfr.inject.BizInfrModule</webModule>
      <webModule>jp.co.slcs.swak.mail.inject.MailModule</webModule>
      <webModule>yuyu.infr.inject.IwfJpaModule</webModule>
      <webModule>jp.co.slcs.swak.conversion.inject.ConversionModule</webModule>
