    <applicationMessages>application_messages</applicationMessages>

    <additionalMessages>
      <additionalMessage>application_messages_base</additionalMessage>
      <additionalMessage>application_messages_biz</additionalMessage>
      <additionalMessage>application_messages_bosyuu</additionalMessage>
      <additionalMessage>application_messages_direct</additionalMessage>
      <additionalMessage>application_messages_dsweb</additionalMessage>
      <additionalMessage>application_messages_hozen</additionalMessage>
      <additionalMessage>application_messages_nenkin</additionalMessage>
      <additionalMessage>application_messages_siharai</additionalMessage>
      <additionalMessage>application_messages_sinkeiyaku</additionalMessage>
      <additionalMessage>application_messages_suuri</additionalMessage>
      <additionalMessage>application_messages_workflow</additionalMessage>
    </additionalMessages>

    <applicationResources>application_resources</applicationResources>

    <additionalResources>
      <additionalResource>application_resources_base</additionalResource>
      <additionalResource>application_resources_biz</additionalResource>
      <additionalResource>application_resources_bosyuu</additionalResource>
      <additionalResource>application_resources_direct</additionalResource>
      <additionalResource>application_resources_dsweb</additionalResource>
      <additionalResource>application_resources_hozen</additionalResource>
      <additionalResource>application_resources_nenkin</additionalResource>
      <additionalResource>application_resources_qrcode</additionalResource>
      <additionalResource>application_resources_siharai</additionalResource>
      <additionalResource>application_resources_sinkeiyaku</additionalResource>
      <additionalResource>application_resources_suuri</additionalResource>
      <additionalResource>application_resources_workflow</additionalResource>
      <additionalResource>application_resources_workflowcore</additionalResource>
      <additionalResource>application_resources_zdb</additionalResource>
    </additionalResources>

    <webModules>
